{
    "name": "kyrne/websocket",
    "description": "Integrated Pusher replacement. Fix for LaForeria.com",
    "keywords": [
        "extension",
        "flarum", "kyrne",
        "websocket", "pusher", "realtime"
    ],
    "type": "flarum-extension",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Charlie Kern",
            "email": "charlie.k@redevs.org",
            "homepage": "https://redevs.org"
        },
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel@klabbers.email",
            "homepage": "http://luceos.com"
        }
    ],
    "support": {
        "forum":  "https://discuss.flarum.org/d/23473-websockets-locally-hosted-alternative-for-pusher",
        "docs": "https://extiverse.com/extension/kyrne/websocket"
    },
    "require": {
        "flarum/core": "^1.0.2",
        "pusher/pusher-php-server": "*",
        "cboden/ratchet": "^0.4.1",
        "clue/redis-react": "^2.3",
        "doctrine/dbal": "^2.9",
        "evenement/evenement": "^2.0|^3.0",
        "guzzlehttp/promises": "^1.0",
        "facade/ignition-contracts": "^1.0",
        "guzzlehttp/psr7": "^1.5|^2.0",
        "illuminate/broadcasting": "^8.0",
        "illuminate/console": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/queue": "^8.0",
        "illuminate/routing": "^8.0",
        "illuminate/support": "^8.0",
        "react/promise": "^2.0",
        "symfony/http-kernel": "^4.0|^5.0",
        "symfony/psr-http-message-bridge": "^1.1|^2.0"
    },
    "autoload": {
        "psr-4": {
            "Kyrne\\Websocket\\": "src/",
            "BeyondCode\\LaravelWebSockets\\": "BeyondCode/src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "replace": {
        "flarum/pusher": "*"
    },
    "extra": {
        "flarum-extension": {
            "title": "Websocket",
            "icon": {
                "background": "url(data:image/svg+xml;base64,) no-repeat, linear-gradient(90deg, rgba(2,0,36,1) 0%, rgba(224,64,251,1) 0%, rgba(33,150,243,1) 100%)",
                "backgroundPosition": "center",
                "backgroundSize": "2em"
            },
            "category": "feature"
        }
    }
}
